{
  "public": true,
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/public/sitemap.xml"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "http://www.aivensystems.com/(.*)",
      "destination": "https://aivensystems.com/$1",
      "permanent": true
    },
    {
      "source": "http://aivensystems.com/(.*)",
      "destination": "https://aivensystems.com/$1",
      "permanent": true
    },
    {
      "source": "/www/(.*)",
      "destination": "https://aivensystems.com/$1",
      "permanent": true
    }
  ]
}
